end;

sub log_trial1 begin
	chlog.append("\t"+ string(correct_stim)+"\t"+string(0)+"\t"+string(0)+"\t"+string(RT)+"\t"+string(RT2)+"\t"+string(1)+"\t\t\t\t"+string(ITItime)+"\t"+string(time_stim)+"\t"+string(time_resp));
end;

